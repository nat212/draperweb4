{
  "printWidth": 80,
  "quoteProps": "as-needed",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "singleQuote": true,
  "arrowParens": "always",
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "angular"
      }
    }
  ],
  "htmlWhitespaceSensitivity": "ignore",
  "proseWrap": "preserve",
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "all"
}
